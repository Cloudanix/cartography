{
  "family": "inventorysyncawsservice",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::774118602354:role/InventorySyncAWSCoreStack-EcsServiceRole-1AZOJK0K8Q449",
  "taskRoleArn": "arn:aws:iam::774118602354:role/InventorySyncAWSCoreStack-ECSTaskRole-EIHL3336LXRO",
  "containerDefinitions": [
    {
      "name": "InventorySyncAWS-Service",
      "image": "774118602354.dkr.ecr.us-east-2.amazonaws.com/inventory-sync-aws:latest",
      "portMappings": [
        {
          "containerPort": 7000,
          "protocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "inventorysyncaws-logs",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "awslogs-inventorysyncaws-service"
        }
      },
      "essential": true
    }
  ]
}
